{
  "name": "multi-site-scraper",
  "version": "1.0.0",
  "description": "Modular multi-site web scraper with pipeline architecture - supports multiple sites with unified CLI",
  "main": "dist/cli/index.js",
  "scripts": {
    "start": "node dist/cli/index.js",
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsc",
    "scrape": "npm run build && node dist/cli/index.js",
    "scrape:dev": "tsx src/cli/index.ts",
    "eventick": "npm run build && node dist/cli/eventick.js",
    "eventick:dev": "tsx src/cli/eventick.ts",
    "test:eventick": "tsx src/cli/test-eventick.ts",
    "extract:all": "tsx src/cli/extract-all-eventick.ts",
    "test": "echo 'no tests'"
  },
  "dependencies": {
    "puppeteer": "^21.5.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "p-limit": "^4.0.0",
    "csv-writer": "^1.6.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2",
    "tsx": "^3.14.0"
  },
  "keywords": [
    "scraping",
    "crawler",
    "puppeteer", 
    "multi-site",
    "pipeline",
    "typescript",
    "web-scraping"
  ],
  "author": "Your Name",
  "license": "MIT"
}
